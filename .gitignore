# Create .gitignore file
cat > .gitignore << 'EOF'
# Dependencies
node_modules/
*/node_modules/

# Environment variables
.env
.env.local
.env.*.local

# Build outputs
dist/
build/
*/dist/
*/build/

# Logs
*.log
logs/

# Database
*.db
*.sqlite
*.db-wal
*.db-shm

# Uploads and temporary files
uploads/
temp/
tmp/

# IDE
.vscode/
.idea/

# OS
.DS_Store
Thumbs.db

# Cache
.cache/
.npm/
.eslintcache

# AI/ML specific
models/
datasets/
training-data/
checkpoints/
*.pth
*.pkl
*.h5

# Blockchain
cache/
artifacts/
EOF